<!DOCTYPE html>
<html lang="en" dir="rtl">

<head>
  <%-include('const/header')%>
</head>

<body>
  <div id="preAuth" class="login">
    <div class="login-body">
      <div class="login-form">
        <form action="/adminPanel/login" method="POST" autocomplete="off">
          <div class="form-group">
            <label>Username</label>
            <input autocomplete="off" name="username" class="form-control" type="text" />
          </div>
          <div class="form-group">
            <label>Password</label>
            <input autocomplete="off" name="password" class="form-control" type="text" />
          </div>
          <button class="btn btn-primary btn-block" type="submit">
            Login
          </button>
        </form>
      </div>
    </div>
  </div>

  <%-include('const/script')%>
</body>

</html>