<div style="display: none;" id="setPassword" class="login">
  <div class="login-body">
    <div class="login-form">
      <form action="javascript:void(0);" autocomplete="off">
        <div class="form-group">
          <label>First Name</label>
          <input
            autocomplete="off"
            id="fname"
            class="form-control"
            type="text"
          />
        </div>
        <div class="form-group">
          <label>Last Name</label>
          <input
            autocomplete="off"
            id="lname"
            class="form-control"
            type="text"
          />
        </div>
        <div class="form-group">
          <label>Password</label>
          <input
            autocomplete="off"
            id="password"
            class="form-control"
            type="text"
          />
        </div>
        <button
          id="setPasswordSubmit"
          class="btn btn-primary btn-block"
          type="button"
        >
          SignUp
        </button>
      </form>
    </div>
  </div>
</div>
